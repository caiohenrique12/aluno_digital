<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="main-login">
  <!-- <img src="lib/images/logo.png" class="img-responsive"> -->
  <div class="panel panel-default">
    <div class="panel-body">
      <form action="index.html">
        <div class="container-fluid">
          <div class="form-group">
            <label for="email">E-mail</label>
            <%= f.email_field :email, class: 'form-control', placeholder: 'Email' %>
          </div>

          <div class="form-group">
            <label for="senha">Senha</label>
            <%= f.password_field :password, autocomplete: "off", class: 'form-control', placeholder: 'Senha' %>
          </div>
          <% if devise_mapping.rememberable? -%>
          <div class="form-group">
            <%= f.check_box :remember_me %>
            <label for="user_remember_me">Continuar Conectado</label>
          </div>
          <% end %>
          <div class="form-group">
            <%= f.submit "Entrar", class: 'btn btn-success pull-right' %>
            <!-- <button class="btn btn-success pull-right"><i class="fa fa-lock"></i> Entrar</button> -->
          </div>
        </div>
      </form>

    </div>
  </div>
<% end %>
